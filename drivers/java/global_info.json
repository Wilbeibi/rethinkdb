{
    "global_optargs": {
        "use_outdated": {
            "type": "R_BOOL",
            "default": false
        },
        "time_format": {
            "type": ["raw", "native"],
            "default": "native"
        },
        "profile": {
            "type": "R_BOOL",
            "default": false
        },
        "durability": {
            "type": ["hard", "soft"],
            "default": "hard"
        },
        "group_format": {
            "type": ["raw", "native"],
            "default": "native"
        },
        "noreply": {
            "type": "R_BOOL",
            "default": false
        },
        "db": {
            "type": "R_STR"
        },
        "array_limit": {
            "type": "R_NUM"
        },
        "binary_format": {
            "type": ["raw", "native"]
        },
        "min_batch_rows": {
            "type": "R_NUM"
        },
        "max_batch_rows": {
            "type": "R_NUM"
        },
        "max_batch_bytes": {
            "type": "R_NUM"
        },
        "max_batch_seconds": {
            "type": "R_NUM"
        },
        "first_batch_scaledown_factor": {
            "type": "R_NUM"
        }
    },
    "exception_hierarchy": {
        "reql_error": {
            "reql_compile_error": {},
            "reql_runtime_error": {
                "reql_query_logic_error": {
                    "reql_non_existence_error": {}
                },
                "reql_resource_limit_error": {},
                "reql_user_error": {},
                "reql_internal_error": {},
                "reql_availability_error": {
                    "reql_op_failed_error": {},
                    "reql_op_indeterminate_error": {}
                }
            },
            "reql_driver_error": {
                "reql_auth_error": {}
            }
        }
    }
}
